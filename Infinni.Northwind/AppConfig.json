{
  "$schema": "https://raw.githubusercontent.com/InfinniPlatform/InfinniPlatform/master/Files/Config/Schema/appConfig.json",
  "app": {
    "AppName": "Northwind",
    "AppInstance": "",
    "ServerName": "localhost",
    "ServerPort": 9900,
    "EmbeddedResourceMapping": {},
    "StaticFilesMapping": {
      "/metadata": "content/metadata",
      "/": "content/www"
    }
  },
  "auth": {
    "LoginPath": "/Auth/SignInInternal",
    "LogoutPath": "/Auth/SignOut",
    "UserCacheTimeout": 30
  },
  "fileSystemBlobStorage": {
    "BaseDirectory": "${INFINNI_BLOBSTORAGE=BlobStorage}"
  },
  "mongodbDocumentStorage": {
    "Nodes": [
      "localhost:27017"
    ]
  },
  "printView": {
    "TempDirectory": "",
    "WkHtmlToPdfArguments": "",
    "WkHtmlToPdfPath": ""
  },
  "quartzScheduler": {
    "ExpireHistoryAfter": null
  },
  "rabbitmqMessageQueue": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "ManagementApiPort": 15672,
    "PrefetchCount": 1,
    "MaxConcurrentThreads": 200,
    "ReconnectTimeout": 5
  },
  "redisSharedCache": {
    "Host": "localhost",
    "Port": 6379,
    "Database": 0,
    "Password": "TeamCity",
    "ReadBufferSize": 512,
    "WriteBufferSize": 512,
    "ConnectionTimeout": 1000,
    "MaxReconnectRetries": 10,
    "PoolSize": 0
  }
}